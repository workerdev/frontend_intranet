extends ../layout
block style_propio
    style.
        body.tinybackground{
            background-color: blue;
            background: blue;
        }

        @media (min-width: 768px) {
            #contenido {
                max-height: 817px;
                overflow-y: auto;
            }
        }
block contenido
    h2.color_texto_base RESPONSABILIDAD SOCIAL
    .container    
        #contenido3(style='background-color:transparent;')
            .row
                each data in responsabilidad
                    .col-6
                        .card
                            .card-body
                                if(data != null || data != undefined)
                                    =data.contenido
                                else
                                    |Sin Datos

